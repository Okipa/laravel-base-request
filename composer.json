{
    "name": "okipa/laravel-request-sanitizer",
    "description": "Easily sanitize your request inputs.",
    "keywords": [
        "okipa",
        "package",
        "php",
        "laravel",
        "request",
        "base",
        "clean",
        "sanitize",
        "sanitizer"
    ],
    "homepage": "https://github.com/Okipa/laravel-sanitizer",
    "license": "MIT",
    "authors": [
        {
            "name": "Arthur LORENT",
            "email": "arthur.lorent@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/support": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0",
        "okipa/php-data-sanitizer": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.8.0|^4.0",
        "squizlabs/php_codesniffer": "^3.3",
        "mockery/mockery": "^1.0",
        "phpmd/phpmd": "^2.0",
        "nunomaduro/larastan": "^0.4"
    },
    "autoload": {
        "psr-4": {
            "Okipa\\LaravelRequestSanitizer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Okipa\\LaravelRequestSanitizer\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/phpcbf",
            "vendor/bin/phpcs",
            "vendor/bin/phpmd src text phpmd.xml",
            "vendor/bin/phpstan analyse",
            "vendor/bin/phpunit"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Okipa\\LaravelRequestSanitizer\\RequestSanitizerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
